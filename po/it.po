msgstr "Plugin '%-.192s' non caricato"

#: drizzled/drizzled_error_messages.h:1081
#, c-format
msgid "Incorrect %-.32s value: '%-.128s'"
msgstr "Errato %-.32s valore: '%-.128s'"

#: drizzled/drizzled_error_messages.h:1083
#, c-format
