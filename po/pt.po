msgid ""
"\n"
"--\n"
"-- Dumping data for table %s\n"
"--\n"
msgstr ""
"\n"
"--\n"
"-- Exportando dados da tabela %s\n"
"--\n"

msgid "-- Retrieving rows...\n"
msgstr "-- Obtendo filas..\n"

