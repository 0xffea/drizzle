#: client/drizzle.cc:1453
#, c-format
msgid "Usage: %s [OPTIONS] [database]\n"
msgstr "Utilisations: %s [OPTIONS][base_de_donnée]\n"

#: client/drizzle.cc:1486
msgid "DELIMITER cannot contain a backslash character"
msgstr "DELIMITER ne peut contenir le caractère backslash"

#: client/drizzle.cc:1507
#, c-format
msgid "WARNING: option deprecated; use --disable-tee instead.\n"
msgstr "ATTENTION: option obsolète; utilisez --disable-tee à la place.\n"

#: client/drizzle.cc:1530
#, c-format
msgid "WARNING: option deprecated; use --disable-pager instead.\n"
msgstr "ATTENTION: option obsolète; utilisez --disable-pager à la place.\n"

#: client/drizzle.cc:1534
#, c-format
msgid "WARNING: --server-arg option not supported in this configuration.\n"
msgstr ""
"ATTENTION: l'option --server-arg n'est pas supportée dans cette "
"configuration.\n"

#: client/drizzle.cc:1902
#, c-format
msgid "Unknown command '\\%c'."
msgstr "Commande inconnue '%c'."

#: client/drizzle.cc:2329
msgid ""
"Reading table information for completion of table and column names\n"
"    You can turn off this feature to get a quicker startup with -A\n"
"\n"
msgstr ""
"Lecture des informations de la table pour compléter les noms de table et de "
"colonne\n"
"    Vous pouvez désactiver cette fonctionnalité pour un démarrage plus "
"rapide avec -A\n"
"\n"

#: client/drizzle.cc:2426
msgid "No connection. Trying to reconnect..."
msgstr "Pas de connexion. Tentative de reconnexion..."

#: client/drizzle.cc:2432
msgid "Can't connect to the server\n"
msgstr "Connexion au serveur impossible\n"

#: client/drizzle.cc:2489
msgid "categories:"
msgstr "catégories:"

#: client/drizzle.cc:2489
msgid "topics:"
msgstr "sujets:"

#: client/drizzle.cc:2540
#, c-format
msgid "Name: '%s'\n"
msgstr "Nom: '%s'\n"

#: client/drizzle.cc:2541
#, c-format
msgid ""
"Description:\n"
"%s"
msgstr ""
"Description:\n"
"%s"

#: client/drizzle.cc:2543
#, c-format
msgid ""
"Examples:\n"
"%s"
msgstr ""
"Exemples:\n"
"%s"

#: client/drizzle.cc:2556
msgid "Many help items for your request exist."
msgstr "Plusieurs entrées dans l'aide existent pour votre requête."

#: client/drizzle.cc:2557
msgid ""
"To make a more specific request, please type 'help <item>',\n"
"where <item> is one of the following"
msgstr ""
"Pour effectuer une demande plus spécifique, tapez 'help <item>',\n"
"où <item> est l'un de ceux qui suivent"

#: client/drizzle.cc:2563
#, c-format
msgid "You asked for help about help category: '%s'\n"
msgstr "Vous avez demandé de l'aide à propos de la catégorie: '%s'\n"

#: client/drizzle.cc:2564
msgid ""
"For more information, type 'help <item>', where <item> is one of the "
"following"
msgstr ""
"Pour plus d'information, tapez 'help <item>', où <item> est l'un de ceux qui "
"suivent"

#: client/drizzle.cc:2577
msgid ""
"\n"
"Nothing found"
msgstr ""
"\n"
"Aucun résultat"

#: client/drizzle.cc:2578
msgid "Please try to run 'help contents' for a list of all accessible topics\n"
msgstr ""
"Essayez d'exécuter 'help contents' pour une liste de tous les sujets "
"accessibles.\n"

#: client/drizzle.cc:2600
msgid "List of all Drizzle commands:"
msgstr "Liste toutes les commandes Drizzle"

#: client/drizzle.cc:2602
msgid "Note that all text commands must be first on line and end with ';'"
msgstr ""
"Notez que toute commande texte doit être en premier sur la ligne et se "
"terminer par ';'"

#: client/drizzle.cc:2613
msgid ""
"\n"
"For server side help, type 'help contents'\n"
msgstr ""
"\n"
"Pour une aide coté serveur, tapez 'help contents'\n"

#: client/drizzle.cc:2656
msgid "No query specified\n"
msgstr "Pas de requête spécifiée\n"

#: client/drizzle.cc:2671
msgid "Ignoring query to other database"
msgstr "Ignore les requêtes faites à une autre base de données"

#: client/drizzle.cc:2720
msgid "Empty set"
msgstr "Résultat vide"

#: client/drizzle.cc:2733
#, c-format
msgid "%ld row in set"
msgid_plural "%ld rows in set"
msgstr[0] "%ld ligne dans le résultat"
msgstr[1] "%ld lignes dans les résultat"

#: client/drizzle.cc:2742
msgid "Query OK"
msgstr "Requête OK"

