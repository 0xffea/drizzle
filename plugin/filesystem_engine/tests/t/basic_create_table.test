--disable_warnings
DROP TABLE IF EXISTS t1;
--enable_warnings

# test file not existed
--error ER_CANT_CREATE_TABLE
CREATE TABLE t1 (a INT) ENGINE=FILESYSTEM,FILE="../../filesystem_ln/_FILE_NOT_EXISTED_";

# test col separator not empty
--error ER_UNKNOWN_ENGINE_OPTION
CREATE TABLE t1 (a INT) ENGINE=FILESYSTEM,FILE="../../filesystem_ln/basic_create_table.data",COL_SEPARATOR="";

# test row separator not empty
--error ER_UNKNOWN_ENGINE_OPTION
CREATE TABLE t1 (a INT) ENGINE=FILESYSTEM,FILE="../../filesystem_ln/basic_create_table.data",ROW_SEPARATOR="";

CREATE TABLE t1 (a INT) ENGINE=FILESYSTEM,FILE="../../filesystem_ln/basic_create_table.data";

# test table existed
--error ER_TABLE_EXISTS_ERROR
CREATE TABLE t1 (a INT) ENGINE=FILESYSTEM,FILE="../../filesystem_ln/basic_create_table.data";

SHOW CREATE TABLE t1;

DROP TABLE t1;
