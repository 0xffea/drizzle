## Generated by register_plugins.py
EXTRA_DIST += plugin/filtered_replicator/pandora-plugin.ac

${top_srcdir}/plugin/filtered_replicator/pandora-plugin.am: ${top_srcdir}/config/register_plugins.py plugin/filtered_replicator/.plugin.ini.stamp

${top_srcdir}/plugin/filtered_replicator/pandora-plugin.ac: ${top_srcdir}/config/register_plugins.py plugin/filtered_replicator/.plugin.ini.stamp

configure: ${top_srcdir}/plugin/filtered_replicator/pandora-plugin.ac

# Prevent errors when a plugin dir is removed
plugin/filtered_replicator/plugin.ini:

# Failures to update the plugin.ini are ignored to permit plugins to be deleted
# cleanly.
${top_srcdir}/plugin/filtered_replicator/.plugin.ini.stamp: plugin/filtered_replicator/plugin.ini
	@if [ ! -e ${top_srcdir}/plugin/filtered_replicator/plugin.ini ]; then \
	    echo "plugin/filtered_replicator/plugin.ini is missing"; \
	else \
	    cmp -s $< $@; \
	    if [ $$? -ne 0 ]; then \
	      echo 'cd ${srcdir} && python config/register_plugins.py ${top_srcdir} plugin/filtered_replicator'; \
	      unchanged=`cd ${srcdir} && python config/register_plugins.py ${top_srcdir} plugin/filtered_replicator` ;\
	      if [ $$? -ne 0 ]; then \
	        echo "**** register_plugins failed ****"; \
	        false; \
	      fi && \
	      for plugin_dir in $$unchanged; do \
	        echo "plugin $$plugin_dir unchanged." ; \
	        touch -r $$plugin_dir/pandora-plugin.am $$plugin_dir/.plugin.ini.stamp; \
	      done && \
              cp -a $< $@ || echo "Failed to update $@"; \
	    fi; \
	fi

plugin_libfiltered_replicator_dir=${top_srcdir}/plugin/filtered_replicator
EXTRA_DIST += plugin/filtered_replicator/plugin.ini
if BUILD_FILTERED_REPLICATOR_PLUGIN
  noinst_LTLIBRARIES+=plugin/libfiltered_replicator_plugin.la
  plugin_libfiltered_replicator_plugin_la_LIBADD=
  plugin_libfiltered_replicator_plugin_la_DEPENDENCIES=
  plugin_libfiltered_replicator_plugin_la_LDFLAGS=$(AM_LDFLAGS) 
  plugin_libfiltered_replicator_plugin_la_CPPFLAGS=$(AM_CPPFLAGS) 
  plugin_libfiltered_replicator_plugin_la_CXXFLAGS=$(AM_CXXFLAGS) 
  plugin_libfiltered_replicator_plugin_la_CFLAGS=$(AM_CFLAGS) 

  plugin_libfiltered_replicator_plugin_la_SOURCES= plugin/filtered_replicator/filtered_replicator.cc
  PANDORA_DYNAMIC_LDADDS+=${top_builddir}/plugin/libfiltered_replicator_plugin.la
endif
noinst_HEADERS +=  plugin/filtered_replicator/filtered_replicator.h
